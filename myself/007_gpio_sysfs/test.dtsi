/ {
	gpio-devs {
		compatible = "org,bone-gpios";
		gpio0 {
			label = "gpio2.2";
			bone-gpio = <&gpio2 2 GPIO_ACTIVE_HIGH>;
		};
		gpio1 {
			label = "gpio2.3";
			bone-gpio = <&gpio2 3 GPIO_ACTIVE_HIGH>;
		};
		gpio2 {
			label = "gpio2.4";
			bone-gpio = <&gpio2 4 GPIO_ACTIVE_HIGH>;
		};
	};
};
